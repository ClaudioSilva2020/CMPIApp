<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.cadastro.db.pendulo.Layout_Setas"
    android:background="@color/switch_thumb_disabled_material_dark">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Direita"
        android:layout_marginEnd="46dp"
        android:src="@drawable/ic_arrow_forward_black_24dp"
        android:layout_centerVertical="true"
        android:layout_alignParentEnd="true" />
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:rotation="180"
        android:id="@+id/Esquerda"
        android:src="@drawable/ic_arrow_forward_black_24dp"
        android:layout_centerVertical="true"
        android:layout_toStartOf="@+id/Direita" />
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:rotation="180"
        android:id="@+id/Tras"
        android:src="@drawable/ic_arrow_upward_black_24dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/Frente"
        android:layout_marginBottom="32dp" />
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Frente"
        android:src="@drawable/ic_arrow_upward_black_24dp"
        android:layout_above="@+id/Tras"
        android:layout_alignParentStart="true"
        android:layout_marginStart="77dp" />

</RelativeLayout>
